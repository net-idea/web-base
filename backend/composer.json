{
  "name": "net-idea/web-base-backend",
  "description": "Shared backend library for Symfony projects - reusable controllers, services, and templates",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Ibrom",
      "email": "adam@net-i.de"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.11|^3.0",
    "doctrine/doctrine-migrations-bundle": "^3.3|^4.0",
    "doctrine/orm": "^2.17|^3.0",
    "symfony/asset": "^6.4|^7.0|^8.0",
    "symfony/console": "^6.4|^7.0|^8.0",
    "symfony/doctrine-messenger": "^6.4|^7.0|^8.0",
    "symfony/dotenv": "^6.4|^7.0|^8.0",
    "symfony/expression-language": "^6.4|^7.0|^8.0",
    "symfony/form": "^6.4|^7.0|^8.0",
    "symfony/framework-bundle": "^6.4|^7.0|^8.0",
    "symfony/http-client": "^6.4|^7.0|^8.0",
    "symfony/mailer": "^6.4|^7.0|^8.0",
    "symfony/mime": "^6.4|^7.0|^8.0",
    "symfony/monolog-bundle": "^3.10",
    "symfony/property-access": "^6.4|^7.0|^8.0",
    "symfony/property-info": "^6.4|^7.0|^8.0",
    "symfony/rate-limiter": "^6.4|^7.0|^8.0",
    "symfony/routing": "^6.4|^7.0|^8.0",
    "symfony/security-bundle": "^6.4|^7.0|^8.0",
    "symfony/serializer": "^6.4|^7.0|^8.0",
    "symfony/string": "^6.4|^7.0|^8.0",
    "symfony/translation": "^6.4|^7.0|^8.0",
    "symfony/twig-bundle": "^6.4|^7.0|^8.0",
    "symfony/validator": "^6.4|^7.0|^8.0",
    "symfony/yaml": "^6.4|^7.0|^8.0",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "erusev/parsedown": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6|^10.0|^11.0|^12.0",
    "symfony/browser-kit": "^6.4|^7.0|^8.0",
    "symfony/css-selector": "^6.4|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "NetIdea\\WebBase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NetIdea\\WebBase\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
